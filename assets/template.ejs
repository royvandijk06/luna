<!DOCTYPE html>
<html>

    <head>
        <meta charset="utf-8">
        <title>
            <%= title %>
        </title>
        <script src="<%= paths.cy %>"></script>
        <script src="<%= paths.cyCollapse %>"></script>
        <script src="<%= paths.dagre %>"></script>
        <script src="<%= paths.cyDagre %>"></script>
        <link rel="stylesheet" href="<%= paths.css %>">
    </head>

    <body>
        <header>
            <h1>[LUNA] <%= title %>
            </h1>
            <div id="settings">
                <h3>Graph</h3>
                <input type="checkbox" id="bShowData" name="bShowData" checked></input>
                <label for="bShowData">Show data (JSON) imports</label>
                <br><br>
                <center>
                    <button id="imgBtn">&#x25EA;</button>
                    <button id="fitBtn">Fit</button>
                    <button id="resetBtn">Center</button>
                    <button id="backBtn" style="display: none;">Back</button>
                </center>
            </div>
            <div id="info" style="display: none;">
                <h3>Node</h3>
                <div id="info-content"></div>
                <center>
                    <button id="lockBtn">&#128275;&#xFE0E;</button>
                    <button id="centerBtn">Center</button>
                    <button id="highlightBtn">Highlight</button>
                </center>
        </header>
        <main>
            <img id="loader" src="<%= paths.loader %>"></img>
            <div id="cy" data-cy="<%= JSON.stringify(data) %>"></div>
            <script>window.data = JSON.parse(document.querySelector("#cy").dataset.cy);</script>
            <script src="<%= paths.cySetup %>"></script>
        </main>
    </body>

</html>